RewriteEngine on
RewriteBase /

##########################################################################################
##################################### SIN WWW A WWW  #####################################
##########################################################################################

RewriteCond %{HTTP_HOST} ^testing2.eu [NC]
RewriteRule ^(.*)$ https://www.testing2.eu/$1 [L,R=301]


##########################################################################################
########################################### CATALA #######################################
##########################################################################################

RewriteRule ^ca/$                                                                                           index.php$1 [L]

RewriteRule ^ca/avis.html(.*)$             		                                                            aviso.php$1 [L]
RewriteRule ^ca/cookies.html(.*)$           		                                                        cookies.php$1 [L]
RewriteRule ^ca/privacitat.html(.*)$        		                                                        privacidad.php$1 [L]
RewriteRule ^ca/sitemaps.html(.*)$        		                                                            sitemaps.php$1 [L]
RewriteRule ^ca/accessibilitat.html(.*)$                                                                    accesibilidad.php$1 [L]

RewriteRule ^ca/contacte.html(.*)$          		                                                        contacto.php$1 [L]

RewriteRule ^ca/bat/MailHandler.php(.*)$                                                                    bat/MailHandler.php [L]
RewriteRule ^ca/bat/MailHandlerPhone.php(.*)$                                                               bat/MailHandlerPhone.php [L]
RewriteRule ^ca/bat/MailHandlerHome.php(.*)$                                                                bat/MailHandlerHome.php [L]


